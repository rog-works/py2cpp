{%- if decorators -%}
	{%- include 'decorator.j2' -%}
{%- endif -%}
{{access}}: static {{return_type}} {{ function_name }}(
	{%- for parameter in parameters -%}
		{%- if loop.index0 > 1 %}, {% endif -%}
		{%- if parameter.symbol != 'cls' -%}
			{{- parameter.variable_type }} {{ parameter.symbol }}{% if parameter.default_value %} = {{ parameter.default_value }}{% endif -%}
		{%- endif -%}
	{%- endfor -%}
) {
{%- filter indent('\t') %}
{{ block }}
{%- endfilter %}
}
