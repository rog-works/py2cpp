{%- for decorator in decorators -%}
{{ decorator }}
{% endfor -%}
{{ access }}: {{ class_symbol }}(
	{%- for parameter in parameters -%}
		{%- if loop.index0 > 1 %}, {% endif -%}
		{%- if parameter != 'this' -%}
			{{- parameter -}}
		{%- endif -%}
	{%- endfor -%}
) {
{%- filter indent('\t') %}
{{ block }}
{%- endfilter %}
}
